[Unit]
Description=librespot
After=network.target

[Service]
ExecStart={PATH_TO_LIBRESPOT_EXEC} --name SandBox -u "{SPOTIFY_USERNAME}" -p "{SPOTIFY_PASSWORD}"
WorkingDirectory={PATH_TO_LIBRESPOT_EXEC_DIR}
StandardOutput=inherit
StandardError=inherit
Restart=always
User={USERNAME}

[Install]
WantedBy=multi-user.target
